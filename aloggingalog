osc(6,-0.015,0.3).diff(osc(6,0.08).rotate(Math.PI/2,.2))
	//.modulateScale(noise(.35,0.25)//.modulateScale(osc(150).rotate(),06)
	.color(1,0.5,0.4).contrast(1.4)
	.add(src(o0).modulate(o0,.04),.6)
    .pixelate(100,100)
	//.invert().brightness(0.1).contrast(1.2)
	.modulateScale(osc(20),-0.2)
  .out(o0)
