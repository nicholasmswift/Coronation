//convoccess
//blackfish
//conversuccess_fish

osc(100,0.1,1.5)
  .color(1,1,0)
  .invert()
  .pixelate(100,100)
  .kaleid(2)
  .modulate(voronoi(10,0.1))
  .blend(o0).blend(o0)
  .out(o0)
shape(4)
  .rotate(0.79)
  .scale(1.7)
  //.modulate(noise(.2,0.1))
  .diff(osc(40,0))
  .out(o1)
src(o0)
  .mult(o1)
  .out(o2)
render(o2)
