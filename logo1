// coronation logo
shape(200)
  .diff(shape(200).scale(0.7))
  //.diff(shape(200).scale(0.8))
  .add(shape(4).scale(0.5,1,0.2).scrollX(0.12).scrollY(0.03))
  .add(shape(4).scale(0.5,1,0.2).scrollX(0.12).scrollY(-0.03))
  .out(o0)
shape(3)
  .scale(0.7,1,1)
  .rotate(-0.53)
  .scale(.9,.7,1.5)
  .scrollX(-0.15)
  .out(o1)
src(o0).mult(o1).out(o2)
src(o0)
  .diff(o2)
  .scrollX(-0.02)
  .scale(2)
  .diff(osc(100).colorama())
  .out(o3)
render()
