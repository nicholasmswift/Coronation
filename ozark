//blckshp

voronoi(100,2)
  .color(0.8,0.8,0.8)
  .add(shape(4).scale(2).modulateRotate(osc(5)))
  //.diff(o3)
  //.modulateScale(osc(4).kaleid(10))
  .out(o0)
osc(100,-.1,2)
  .color(0,1,1)
  //.colorama()
  .pixelate(50,50)
  .rotate(0,0.5)
  .modulateScale(voronoi(10).kaleid())
  //.kaleid(6)
  .out(o1)
src(o1)
  .mult(o0)
  .contrast(3)
  .blend(o2).blend(o2)//.blend(o2)//.blend(o2)
  //.rotate(0,0.5)
  .out(o2)
render()
