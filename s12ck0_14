osc(6, -0.2, 0.3)
  .modulate(noise(100).color(0.5,0.5,0.5))
  .kaleid(2)
  .rotate()
  //.pixelate()
  .modulateScale(noise(1,.1))
  //.diff(o0)
  .color(1,0.5,0.5)
  .colorama().invert()
  .brightness(-0.1).contrast(1.5)
  .out(o0)
shape(4).scale(2.7).out(o1)
src(o0)
  //.colorama(03)//.color(0.4,0.4,-1.1)
  .mult(o1)
  .blend(o2).blend(o2)//.blend(o2).blend(o2)
  //.mult(o3)
  .out(o2)
render(o2)
