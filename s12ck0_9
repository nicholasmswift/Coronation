osc(4, -0.1, 03)
  .modulate(noise(20))
  .pixelate(50,50)
  .kaleid(10)
  .color(.4,0.4, -1.1)
  //.rotate(0.30, 0.2)
  //.pixelate(2, 20)
  .modulate(noise(3))
  //.modulate(noise(()=>Math.sin(time)*2))
  .out(o0)
shape(4).scale(2.7).out(o1)
src(o0)
  .colorama(03)//.color(0.4,0.4,-1.1)
  .mult(o1)
  .blend(o2).blend(o2)//.blend(o2).blend(o2)
  .out(o2)
render(o2)
