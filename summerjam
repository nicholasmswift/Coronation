voronoi(1,0.3,0.2).shift(0.5)
.modulatePixelate(voronoi(10,0.2),32,2)
.scale(0.5)
//.scale(()=>(.1+Math.sin(time*2.5)*0.05))
//.diff(voronoi(3).shift(0.6))
.diff(osc(2,0.15,1.1).rotate().modulate(noise().modulate(noise())))
.brightness(0.3).contrast(1.2).saturate(1.2)
//.modulate(noise().modulate(noise()))
	.out()
speed = 0.8
